<div class="text-center">
  <h2>{{name}}</h2>
  <p>{{description}}</p>
  <p>Created by {{user.name}} on {{format_date date_created}}</p>
  <p>
     <button class="btn btn-primary">Create this blog!</button>
  </p>
</div>

<div class="card">
  <div class="card-header">
    <h2>
      <a href="/project/{{projectt.id}}">{{project.name}}</a>
    </h2>
    <p>Created by {{project.user.name}} on {{format_date project.date_created}}</p>
  </div>
  <div class="card-body">
    <p>
      {{project.description}}
    </p>
  </div>
</div>